{
  "name": "bwein-net/contao-protected-redirect-element",
  "description": "Protected Redirect Element for Contao Open Source CMS",
  "keywords": [
    "contao",
    "protected",
    "redirect",
    "element",
    "content",
    "password",
    "download"
  ],
  "type": "contao-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "bwein.net",
      "homepage": "https://bwein.net/"
    }
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/rabauss"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "contao/core-bundle": "^4.9",
    "symfony/config": "4.4.* || 5.2.*",
    "symfony/dependency-injection": "4.4.* || 5.2.*",
    "symfony/http-foundation": "4.4.* || 5.2.*",
    "symfony/http-kernel": "4.4.* || 5.2.*",
    "symfony/translation": "4.4.* || 5.2.*"
  },
  "require-dev": {
    "contao/easy-coding-standard": "^3.0",
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "Bwein\\ProtectedRedirectElement\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Bwein\\ProtectedRedirectElement\\ContaoManager\\Plugin"
  },
  "scripts": {
    "cs-fixer": [
      "vendor/bin/ecs check contao/ src/ --fix --ansi",
      "vendor/bin/ecs check contao/templates --config ecs_template.php --fix --ansi"
    ]
  }
}
